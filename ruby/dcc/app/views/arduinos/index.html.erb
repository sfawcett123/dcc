
  <table class="table" >
      <thead>
            <tr>
              <th>Board Name</th>
              <th>Port</th>
              <th>Serial Number </th>
              <th>Product Id </th>
              <th>Vendor Id </th>
            </tr>
      </thead>
      <tbody>
  <% @arduinos.each do |arduino| %>
    <tr>
      <td><%= arduino.fetch('port').fetch('name'         , '') %> </td>
      <td><%= arduino.fetch('port').fetch('label'        , '') %> </td>
      <td><%= arduino.fetch('port').fetch('serialNumber' , '') %> </td>
      <td><%= arduino.fetch('port').fetch('pid'          , '') %> </td>
      <td><%= arduino.fetch('port').fetch('vid'          , '') %> </td>
      <td><a href="#" class="btn btn-success">OK</a></td>
    </tr>
  <% end %>
      </tbody>
  </table>



